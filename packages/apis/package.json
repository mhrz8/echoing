{
  "name": "@echoing/apis",
  "version": "0.0.0",
  "description": "apis package for echoing app",
  "author": "hirzinurfakhrian@gmail.com",
  "type": "module",
  "license": "MIT",
  "private": true,
  "packageManager": "yarn@4.5.3",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*",
    "package.json"
  ],
  "scripts": {
    "build": "buf generate && tsx ./src/scripts/gen-grpc-index.ts && tsc --p tsconfig.json",
    "watch": "concurrently --kill-others --success all --names buf,patch,build \"nodemon\" \"tsx ./src/scripts/gen-grpc-index.ts\" \"tsc --watch --p tsconfig.json\"",
    "check-types": "tsc --noEmit"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.12.5",
    "@protobuf-ts/grpc-transport": "^2.9.4",
    "@protobuf-ts/runtime": "^2.9.4",
    "@protobuf-ts/runtime-rpc": "^2.9.4",
    "case-anything": "^3.1.0"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.48.0",
    "@echoing/eslintconfig": "workspace:*",
    "@echoing/tsconfig": "workspace:*",
    "@protobuf-ts/plugin": "^2.9.4",
    "@types/node": "^22.10.2",
    "concurrently": "^9.1.0",
    "nodemon": "^3.1.9",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
